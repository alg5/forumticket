{
    "name": "alg/forumticket",
    "type": "phpbb-extension",
    "description": "Тип форума,  в котором темы видит только автор темы и определённая группа ",
    "homepage": "https://github.com/alg5/forumticket",
    "version": "1.0.2",
    "time": "2016-06-20",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Alg",
            "homepage": "http://www.phpbbguru.net",
            "role": "Developer"
        }
       
    ],
    "require": {
        "php": ">=5.4.2"
    },
	"require-dev": {
      "phpbb/epv": "dev-master"
   },
	"extra": {
        "display-name": "Forum Ticket",
        "soft-require": {
           "phpbb/phpbb": "3.2.*@dev"
        },
        "version-check": {
            "host": "alg5.github.io",
            "directory": "/versions",
            "filename": "forumticket.json"
        }
    }
}
